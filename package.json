{
  "name": "gatsby-starter-blog",
  "private": true,
  "description": "A starter for a blog powered by Gatsby and Markdown",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^13.2.0",
    "@svgr/cli": "^4.3.2",
    "babel-plugin-styled-components": "^1.10.6",
    "dotenv": "^8.0.0",
    "gatsby": "^2.13.31",
    "gatsby-image": "^2.2.6",
    "gatsby-plugin-feed": "^2.3.4",
    "gatsby-plugin-google-analytics": "^2.1.4",
    "gatsby-plugin-manifest": "^2.2.3",
    "gatsby-plugin-offline": "^2.2.4",
    "gatsby-plugin-react-helmet": "^3.1.2",
    "gatsby-plugin-sharp": "^2.2.8",
    "gatsby-plugin-sitemap": "^2.2.3",
    "gatsby-plugin-styled-components": "^3.1.2",
    "gatsby-plugin-typography": "^2.3.2",
    "gatsby-remark-copy-linked-files": "^2.1.3",
    "gatsby-remark-images": "^3.1.6",
    "gatsby-remark-prismjs": "^3.3.3",
    "gatsby-remark-responsive-iframe": "^2.2.4",
    "gatsby-remark-smartypants": "^2.1.2",
    "gatsby-source-contentful": "^2.1.14",
    "gatsby-source-filesystem": "^2.1.5",
    "gatsby-transformer-remark": "^2.6.7",
    "gatsby-transformer-sharp": "^2.2.4",
    "intersection-observer": "^0.7.0",
    "prismjs": "^1.16.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-google-recaptcha": "^1.1.0",
    "react-helmet": "^5.2.1",
    "react-intersection-observer": "^8.24.1",
    "react-slick": "^0.24.0",
    "react-typography": "^0.16.19",
    "styled-components": "^4.3.2",
    "typeface-merriweather": "0.0.72",
    "typeface-montserrat": "0.0.75",
    "typography": "^0.16.19",
    "typography-theme-wordpress-2016": "^0.16.19"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "eslint": "^5.3.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "npm-run-all": "4.1.5",
    "prettier": "^1.18.2"
  },
  "homepage": "https://github.com/gatsbyjs/gatsby-starter-blog#readme",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gatsbyjs/gatsby-starter-blog.git"
  },
  "scripts": {
    "build": "gatsby build",
    "clean": "rm -fr node_modules package-lock.json",
    "clean:all": "npm run clean:build && npm run clean",
    "clean:build": "rm -fr public .cache",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "icons": "rm -fr src/components/icons && npx @svgr/cli --ext jsx --template src/templates/svgr/icons-template.js -d src/components/icons content/assets/icons",
    "lint": "npm-run-all --continue-on-error -p lint:code lint:style",
    "lint:code": "eslint  --ignore-path .eslintignore --ext .js,.jsx \"src/**\"",
    "lint:style": "prettier --check \"src/**\"",
    "logos": "rm -fr src/components/logos && npx @svgr/cli --ext jsx --template src/templates/svgr/icons-template.js -d src/components/logos content/assets/logo",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\""
  }
}
